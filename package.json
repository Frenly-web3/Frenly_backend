{
  "name": "socialfi-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start": "node dist/src/main",
    "migration:create:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:create ./src/data/migrations/migrations --outputJs",
    "migration:generate:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate ./src/data/migrations/migrations -d ./src/data/datasource.ts --pretty --outputJs",
    "migration:up:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data/datasource.ts",
    "migration:down:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/data/datasource.ts",
    "migration:create:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:create ./src/data/migrations/migrations --outputJs",
    "migration:generate:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate ./src/data/migrations/migrations -d ./src/data/datasource.ts --pretty --outputJs",
    "migration:up:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data/datasource.ts",
    "migration:down:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/data/datasource.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.6",
    "@automapper/core": "^8.7.6",
    "@automapper/nestjs": "^8.7.6",
    "@maticnetwork/maticjs": "^3.4.0",
    "@maticnetwork/maticjs-web3": "^1.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.1.4",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "ethereumjs-util": "^7.1.5",
    "express-http-context": "^1.2.4",
    "moment": "^2.29.4",
    "nft.storage": "^7.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.0",
    "uuid": "^9.0.0",
    "web3": "^1.7.5",
    "web3-eth-contract": "^1.7.5",
    "web3-utils": "^1.7.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/cron": "^2.0.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
